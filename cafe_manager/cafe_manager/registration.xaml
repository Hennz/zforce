        <Label x:Name="lbl_er_email_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,59,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_username_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,92,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_password_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,125,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_confirmpassword_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,158,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_dob_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,188,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_gender_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,221,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_cellphone_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,247,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_city_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,283,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_state_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,316,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_country_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,349,0,0" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="lbl_er_pincode_msg" Content="" HorizontalAlignment="Left" Height="28" Margin="467,382,0,0" VerticalAlignment="Top" Width="311" RenderTransformOrigin="0.537,1.393"/>
        <Button x:Name="btn_register" Content="Register" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="234,428,0,0"/>

    </Grid>
