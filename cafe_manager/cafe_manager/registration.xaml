        <Label x:Name="lbl_pincode" Content="PIN" HorizontalAlignment="Left" Height="28" Margin="23,382,0,0" VerticalAlignment="Top" Width="109" RenderTransformOrigin="0.537,1.393"/>
        <TextBox x:Name="txt_name" HorizontalAlignment="Left" Height="28" Margin="175,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" ToolTip="Enter Your Full Name"/>
        <TextBox x:Name="txt_email" HorizontalAlignment="Left" Height="28" Margin="175,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" ToolTip="Enter Your valid email address"/>
        <TextBox x:Name="txt_username" HorizontalAlignment="Left" Height="28" Margin="175,92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" ToolTip="Choose your username"/>
