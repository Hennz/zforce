        <Label x:Name="lbl_dob" Content="Birthdate" HorizontalAlignment="Left" Height="28" Margin="23,188,0,0" VerticalAlignment="Top" Width="109"/>
        <Label x:Name="lbl_gender" Content="Gender" HorizontalAlignment="Left" Height="28" Margin="23,221,0,0" VerticalAlignment="Top" Width="109"/>
