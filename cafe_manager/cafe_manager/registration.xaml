        <DatePicker x:Name="date_birthdate" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="175,191,0,0" Width="109" SelectedDateFormat="Long"/>
        <RadioButton x:Name="gender_male" Content="Male" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="175,227,0,0" GroupName="Gender"/>
        <RadioButton x:Name="gender_female" Content="Female" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="252,227,0,0" GroupName="Gender"/>
        <TextBox x:Name="txt_cellphone" HorizontalAlignment="Left" Height="28" Margin="175,247,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" ToolTip="Confirm your password"/>
        <TextBox x:Name="txt_cellphone_Copy" HorizontalAlignment="Left" Height="28" Margin="175,283,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" ToolTip="Confirm your password"/>
        <TextBox x:Name="txt_cellphone_Copy1" HorizontalAlignment="Left" Height="28" Margin="175,316,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" ToolTip="Confirm your password"/>
